{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kurs-pm-data-admin-web",
  "version": "2.0.0",
  "description": "Kurs-pm administation tool and editor to create a kurs-pm by filling in data in editor and fetching data from syllabus",
  "main": "app.js",
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kurs-pm-data-admin-web.git"
  },
  "scripts": {
    "vendor": "parcel build  ./public/js/vendor.js",
    "lint": "eslint . && prettier-eslint --list-different **/*.js",
    "test": "NODE_ENV=test jest",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "NODE_ENV=production rm -rf dist && parcel build ./public/js/app/app.jsx && npm run vendor --preserve-comments",
    "build-dev": "cross-env NODE_ENV=development rm -rf dist && parcel build ./public/js/vendor.js --public-url /kursinfoadmin/kurs-pm-data/static && parcel watch ./public/js/app/app.jsx --public-url /kursinfoadmin/kurs-pm-data/static",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run build-dev\" \"nodemon --ignore dist/ --ignore public/ app.js\""
  },
  "dependencies": {
    "@kth/kth-kip-style-react-components": "0.0.22",
    "@tinymce/tinymce-react": "^3.8.1",
    "applicationinsights": "^1.8.8",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "component-registry": "^1.3.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.6",
    "kth-node-access-log": "^0.2.2",
    "kth-node-api-call": "^3.0.12",
    "kth-node-configuration": "^2.0.3",
    "kth-node-express-routing": "^2.0.3",
    "kth-node-i18n": "^1.0.3",
    "kth-node-ldap": "^3.1.1",
    "kth-node-log": "^3.1.3",
    "kth-node-monitor": "^1.1.8",
    "kth-node-passport-cas": "^3.1.12",
    "kth-node-redis": "^3.1.7",
    "kth-node-server": "^3.1.2",
    "kth-node-session": "^1.0.4",
    "kth-node-web-common": "^5.1.4",
    "kth-style": "^5.0.30",
    "locale": "^0.1.0",
    "mobx": "^5.15.7",
    "mobx-react": "^5.4.4",
    "passport": "^0.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^3.11.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^1.2.2",
    "react-sticky": "^6.0.3",
    "reactstrap": "^8.7.1",
    "safe-utils": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^10.4.9",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "merge-stream": "^2.0.0",
    "mockery": "^2.1.0",
    "nock": "^10.0.2",
    "node-mocks-http": "^1.9.0",
    "nodemon": "^2.0.6",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "prettier-eslint-cli": "^5.0.0",
    "pretty-quick": "^2.0.2",
    "react-test-renderer": "^16.14.0",
    "sass": "^1.28.0"
  },
  "engines": {
    "node": "14.0.0"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test"
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none",
    "tabWidth": 2
  }
}
