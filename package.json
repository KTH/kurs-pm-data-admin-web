{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kurs-pm-data-admin-web",
  "version": "4.0.0",
  "description": "Kurs-pm administation tool and editor to create a kurs-pm by filling in data in editor and fetching data from syllabus",
  "main": "app.js",
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kurs-pm-data-admin-web.git"
  },
  "scripts": {
    "cypress:open": "NODE_ENV=development cypress open",
    "prepare": "husky install",
    "lint": "eslint . && prettier-eslint --list-different **/*.js",
    "lint:new": "eslint \"public/**/*.{js,jsx}\" --quiet",
    "test": "NODE_ENV=test jest ./test",
    "test-win": "cross-env NODE_ENV=test jest",
    "test-editor": "NODE_ENV=test jest ./test/unit/EditorIntegration.test.js --watch --notify",
    "test-preview": "NODE_ENV=test jest ./test/unit/PreviewContainerFilledIn.test.js --watch --notify",
    "test-new-memo": "NODE_ENV=test jest ./test/unit/CreateNewMemo.test.js --watch --notify",
    "test-pub-memo": "NODE_ENV=test jest ./test/unit/ChangePublished.test.js --watch --notify",
    "test-edit-new": "NODE_ENV=test jest ./test/unit/MemoContainerNewDraft.test.js --watch --notify",
    "test-edit-published": "NODE_ENV=test jest ./test/unit/MemoContainerPublished.test.js --watch --notify",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:notify": "NODE_ENV=test jest --watch --notify",
    "vendor": "parcel build  ./public/js/vendor.js",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "NODE_ENV=production rm -rf dist && parcel build ./public/js/app/app.jsx && parcel build ./public/js/app/ssr-app.js && npm run vendor --preserve-comments",
    "build-dev:old": "cross-env NODE_ENV=development rm -rf dist && parcel build ./public/js/vendor.js --public-url /kursinfoadmin/kurs-pm-data/static && parcel build ./public/js/app/ssr-app.js && parcel watch ./public/js/app/app.jsx --public-url /kursinfoadmin/kurs-pm-data/static",
    "start": "NODE_ENV=production node app.js",
    "start-dev:old": "cross-env NODE_ENV=development concurrently --kill-others \"npm run build-dev\" \"nodemon --ignore dist/ --ignore public/ app.js\"",
    "build:bash": "bash ./build.sh prod /kursinfoadmin/kurs-pm-data",
    "build-dev": "bash ./build.sh dev /kursinfoadmin/kurs-pm-data",
    "start:bash": "bash -c 'NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@kth/kth-kip-style-react-components": "0.1.2",
    "@kth/kth-node-passport-oidc": "^1.0.43",
    "@tinymce/tinymce-react": "^3.13.0",
    "applicationinsights": "^1.8.10",
    "axios": "^0.21.2",
    "body-parser": "^1.19.1",
    "component-registry": "^1.3.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.2",
    "express-handlebars": "^5.3.5",
    "kth-node-access-log": "^0.2.7",
    "kth-node-api-call": "^3.0.18",
    "kth-node-configuration": "^2.0.8",
    "kth-node-express-routing": "^2.0.12",
    "kth-node-i18n": "^1.0.12",
    "kth-node-log": "^3.1.3",
    "kth-node-monitor": "^1.1.14",
    "kth-node-redis": "^3.1.36",
    "kth-node-server": "^3.1.9",
    "kth-node-session": "^1.0.7",
    "kth-node-web-common": "^5.1.17",
    "kth-style": "^6.0.25",
    "locale": "^0.1.0",
    "mobx": "^6.3.13",
    "mobx-react": "^7.2.1",
    "passport": "^0.4.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-router-hash-link": "^2.4.3",
    "react-sticky": "^6.0.3",
    "reactstrap": "^8.10.1",
    "safe-utils": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-runtime": "^7.13.7",
    "@babel/preset-env": "7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@kth/eslint-config-kth": "^1.0.15",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "chai": "^4.3.4",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.4.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-extended": "^0.11.5",
    "mockery": "^2.1.0",
    "nock": "^13.2.2",
    "node-notifier": "^10.0.1",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.15",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.49.0"
  },
  "engines": {
    "node": "14.2.0"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  }
}
