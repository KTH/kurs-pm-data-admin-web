{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }, "plugin-proposal-decorators"],
    ["@babel/plugin-proposal-class-properties", { "loose": true }, "plugin-proposal-class-properties"],
    ["@babel/plugin-proposal-private-methods", { "loose": true }, "plugin-proposal-private-methods"],
    "@babel/plugin-transform-runtime"
  ]
}