{
  "plugins": ["react"],
  "extends": ["plugin:jsx-a11y/recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "classes": true,
      "impliedStrict": true,
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "16.12.0"
    }
  }
}
